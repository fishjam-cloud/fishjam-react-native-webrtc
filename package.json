{
    "name": "@fishjam-cloud/react-native-webrtc",
    "version": "0.25.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fishjam-cloud/fishjam-react-native-webrtc.git"
    },
    "description": "WebRTC for React Native",
    "license": "MIT",
    "homepage": "https://github.com/fishjam-cloud/fishjam-react-native-webrtc",
    "keywords": [
        "react-component",
        "react-native",
        "ios",
        "android",
        "webrtc"
    ],
    "react-native": "src/index.ts",
    "types": "lib/typescript/index.d.ts",
    "main": "lib/commonjs/index.js",
    "module": "lib/module/index.js",
    "dependencies": {
        "base64-js": "1.5.1",
        "debug": "4.3.4",
        "event-target-shim": "6.0.2"
    },
    "peerDependencies": {
        "react-native": ">=0.60.0"
    },
    "scripts": {
        "lint": "eslint --max-warnings 0 . && tsc --noEmit",
        "lintfix": "eslint --max-warnings 0 --fix . && tsc --noEmit",
        "prepare": "bob build",
        "format": "tools/format.sh && npx prettier --write 'src/**/*.{ts,tsx}' 'metro.config.js' 'metro.config.macos.js' 'react-native.config.js'"
    },
    "bugs": {
        "url": "https://github.com/fishjam-cloud/fishjam-react-native-webrtc/issues"
    },
    "devDependencies": {
        "@types/debug": "4.1.7",
        "@types/react": "19.1.0",
        "react-native": "0.81.5",
        "@typescript-eslint/eslint-plugin": "^8.18.0",
        "@typescript-eslint/parser": "^8.18.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.26.0",
        "prettier": "3.4.2",
        "react-native-builder-bob": "0.18.2",
        "typescript": "~5.9.2"
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            "typescript"
        ]
    }
}
